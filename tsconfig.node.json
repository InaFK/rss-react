{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["node", "jest", "msw", "vite"]
  },
  "include": [
    "vite.config.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
